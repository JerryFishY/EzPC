uint32 dim = 5u ;

def void main () {
  input(SERVER, w, float_al[dim]) ;
  input(SERVER, b, float_al) ;
  input(CLIENT, x, float_al[dim]) ;

  uint32_pl lower = 0u ;
  uint32_pl upper = dim ;

  float_al acc = 0.0 ;
  for i=[lower - lower : upper] {
    acc = acc + (w[i] * x[i]) ;
  } ;

  output(CLIENT, acc > b ? true : false) ;
}
