def float f (float x) {
  return 1.0/x ;
}

def void main () {
  input(CLIENT, l, float_al) ;
  input(CLIENT, r, float_al) ;

  int32_pl terms = 20 ;
  float_pl termsf = 20.0 ; 
  float_al h = (r-l)/termsf ;
  float_al acc = 0.0 ;
  float_al x = l ;
  for i=[0:terms] {
    acc = acc + (f(x) * h) ;
    x = x + h ;
  } ;

  output(CLIENT, acc) ;
}
